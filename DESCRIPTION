Package: biomech
Type: Package
Title: Compute Beam Bending and Torsion
Version: 0.1.0
Authors@R: c(person(given = "Remi", family = "Vezy", email = "remi.vezy@cirad.fr", role = c("aut","cre"),comment = c(ORCID = "0000-0002-0808-1461")),
